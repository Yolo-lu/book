.title{
  display: flex;
  margin: 10rpx 30rpx ;
  border-bottom: 1px solid #f3f3f3;
}
.name{
  padding: 20rpx;
}
.head{
  margin: 10rpx 20rpx ;
  border-bottom: 1px solid #f3f3f3;
  white-space: nowrap;
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden
}
.names{
  padding: 20rpx 16rpx;
  display: inline-block;
}
.book{
  display: flex;
  margin:30rpx ;
   border-bottom: 1px solid #f3f3f3;
}
.icon image{
  width: 200rpx;
  height: 280rpx
}
.right{
  margin-left: 20rpx
}
.right .titles{
  font-weight: bold;
}
.right .author{
  color: #666;
  margin: 10rpx 0;
  font-size: 28rpx
}
.right .authors{
   color: #666;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: wrap;
  width: 450rpx;
  height: 122rpx;
  font-size: 28rpx
}
.tags{
  display: flex;
}
.tag{
  padding: 10rpx;
  background: rgb(245, 12, 12);
  margin-right: 10rpx;
  border-radius: 10rpx;
  color: #fff;
  margin-bottom: 10rpx;
  font-size: 26rpx
}
.color{
background: #00CED1
}
.back{
  background: #FF5E23
}
.footer{
  margin: 60rpx;
  color: #666;
  text-align: center
}
.style{
  color: #cb1c36;
}