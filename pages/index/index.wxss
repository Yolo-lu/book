.head{
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}
image{
  width: 40rpx;
  height: 40rpx;
  display: block;
}
.left{
  margin-left: 60rpx;
  position: relative;
  top: 14rpx;
}
.right{
  display: flex;
  margin-right: 70rpx;
}
.help{
  border: 1px solid #000;
  padding: 10rpx 20rpx;
  margin-right: 40rpx;
  border-radius: 16rpx
}
.new{
  position: relative;
  top: 14rpx;
}
.contanier{
color: #666;
margin-top: 150rpx;
text-align: center;
}
.relative{
   display: flex;
   margin: 40rpx 20rpx;
   flex-wrap: wrap
}
.relativeBook{
width: 33%
}
.relativeRcommend{ 
  margin: 20rpx;
  position: relative
}
.icon{
  position: absolute;
  right: -30rpx;
  top: -30rpx;
  z-index: 99
}
.recommendCover{
  width: 200rpx;
  height: 250rpx;
  display: block;
  margin-bottom: 10rpx
}
.lastRead{
  font-size: 24rpx;
  color: #666;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap
