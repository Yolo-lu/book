@import '../../lib/wxParse/wxParse.wxss';
.read{
  border: 2rpx solid #999;
  padding: 20rpx 40rpx;
  border-radius: 40rpx;
  background: #cb1c36;
  color: #fff;
}
.row{
  display: flex;
  margin: 20rpx 100rpx;
  justify-content: space-around;
  position: fixed;
  bottom: 200rpx;
  left: 0;
  right: 0
}
.all{
  width: 95%;
  margin: auto
}
.title{
  font-size: 36rpx;
  font-weight: bold;
  padding: 40rpx;
  border-bottom: 2rpx solid #f4f4f4
}
.WxEmojiView{
  text-indent: 2em;
  margin-top: 40rpx
}
.wxParse-inline{
  display: block
}